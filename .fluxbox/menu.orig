		[exec] (Gvim) {gvim}
		[exec] (IDLE) {idle}
		[exec] (VirtualBox) {VirtualBox}
		[exec] (Eagle) {/home/hrvi/razno/eagle/bin/eagle}
		[exec] (MPLAB) {wine /home/hrvi/.wine/drive_c/Program\ Files/Microchip/MPLAB\ IDE/Core/MPLAB.exe}
		[exec] (BitCoin) {/home/hrvi/Downloads/bitcoin-0.6.2-linux/bin/64/bitcoin-qt}
	[end]
	[submenu] (Igre) {}
		[exec] (Homeworld) {/home/hrvi/homeworld}

		[exec] (Steam) {/home/hrvi/steam}
		[exec] (Unreal) {wine "/home/hrvi/.wine/drive_c/UnrealTournament/System/UnrealTournament.exe"}
		[exec] (Torchlight) {/usr/bin/wine "/home/hrvi/.wine/drive_c/Torchlight/Torchlight.exe"}
		[exec] (AcesHigh) {/home/hrvi/.wine/drive_c/AcesHigh/aceshigh.exe}
		[exec] (Oolite) {/home/hrvi/Igre/Oolite/oolite}

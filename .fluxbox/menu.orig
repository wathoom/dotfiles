		[exec] (OpenTTD) {/home/hrvi/Igre/openttd/openttd}
		[exec] (FreeOrion) {/home/hrvi/Igre/freeorion/freeorion}
		[exec] (VegaStrike) {/home/hrvi/Igre/vegastrike/bin/vegastrike-64}
		[exec] (Cogs) {/home/hrvi/Igre/cogs/Cogs-amd64}
		[exec] (Hammerfight) {/home/hrvi/Igre/hammerfight/Hammerfight}
		[exec] (SteelStorm) {/home/hrvi/Igre/steelstorm/steelstorm64 -basedir /home/hrvi/Igre/steelstorm/}
	[end]
	[submenu] (Graphics) {}
		[exec] (Comix) {comix}
		[exec] (Image Viewer) {geeqie}
		[exec] (Gimp) {gimp}
		[exec] (PDFEditor) {pdfedit}
		[exec] (XSANE) {xsane}

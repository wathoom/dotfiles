	[end]
	[submenu] (Internet) {}
		[exec] (JDownloader) {java -jar /home/hrvi/JDownloader/JDownloader.jar}
		[exec] (Azureus) {/home/hrvi/vuze/vuze}
		[exec] (GFTP) {gftp}
		[exec] (Opera) {opera}
		[exec] (Chrome) {/opt/google/chrome/google-chrome}
		[exec] (Thunderbird) {thunderbird}
		[exec] (Google Earth) {~/google-earth/googleearth}
		[exec] (Firefox) {firefox}
		[exec] (Pidgin) {pidgin}
		[exec] (WiCD) {wicd-client -n}
		[exec] (Teamspeak2) {aoss ~/TeamSpeak2RC2/TeamSpeak}
		[exec] (Teamspeak3) {/home/hrvi/TeamSpeak3/ts3client_runscript.sh}
	[end]
	[submenu] (Office) {}
		[exec] (LibreOffice) {soffice}
		[exec] (muPDF) {mupdf}
		[exec] (Foxit) {/home/hrvi/razno/FoxitReader/FoxitReader}
		[exec] (Calibre) {calibre}

	[end]
	[submenu] (Sound) {}
		[exec] (XCDRoast) {xcdroast -n}
		[exec] (XMMS) {xmms}
		[exec] (Audacity) {audacity}
	[end]
	[submenu] (Video) {}
		[exec] (Mplayer) {gmplayer}
		[exec] (Xine) {xine}
	[end]
	[submenu] (System) {}
		[exec] (htop) {xterm -bg black -fg green htop}
		[exec] (conky) {conky}
		[config] (Configure)
			[submenu] (System Styles) {Choose a style...}
      				[stylesdir] (/usr/share/fluxbox/styles)
			[end]
			[submenu] (User Styles) {Choose a style...}
      				[stylesdir] (~/.fluxbox/styles)
			[end]

     			[workspaces] (Workspace List)
			
			[submenu] (Tools)
   			   [exec] (Window name) {xprop WM_CLASS|cut -d \" -f 2|xmessage -file - -center}
  			   [exec] (Screenshot - JPG) {import screenshot.jpg && display -resize 50% screenshot.jpg}
   			   [exec] (Screenshot - PNG) {import screenshot.png && display -resize 50% screenshot.png}
    			   [exec] (Regen Menu) {fluxbox-generate_menu }
			[end]
	[end]
	[separator]
	[exec] (Lock screen) {xlock -mode blank}
	[exit] (Exit)
[end]
